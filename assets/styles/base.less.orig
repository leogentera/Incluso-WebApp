/********************************************************************************
    Incluso Digital CSS Base Styles

    Description:    CSS rules for grid system, controls, components, widgets and
    				plugins are defined/overriden in this file.
    Uses:           Bootstrap.css, Modernizer.
    Targets:        Modern Browsers, Android's WebView.
	
	Index:
            01. $incluso-app    
            02. $btn                (bootstrap)
            03. $btn-back-to-top    
            04. $button-group       (bootstrap)    
            05. $form-control       (bootstrap)
            06. $input-group        (bootstrap)
            07. $alert              (bootstrap)
            08. $navbar             (bootstrap)
            09. $navbar-toggle      (bootstrap)
            10. $list-group
            11. $inline-list
            12. $block-list
            13. $component            
            14. $pages
            15. $footer
********************************************************************************/
@import "less-utilities.less";

body {
    display: block;
    position: absolute;
    width: 100%;
    height: inherit;
}

.incluso-app {

    /****************************************************************************
     $incluso-app 
     Description: CSS rules for basic namespace wrapper.
    ****************************************************************************/   
    padding:0; margin:0 auto;
    width: auto; max-width: none;
    font-family: Play, Sans-serif, Arial;
    a{
        &:hover{
            text-decoration: none;
        }
    }
    .container{
        padding:0;
    }
    /***************************************************************************/  


    /****************************************************************************
     $btn
     Description: CSS override to bootstrap's btn class.
    ****************************************************************************/
    .btn {
        border: none;
        .border-radius-all(3px);
        font-weight: 600;        
    }
    .btn-cnt{
        width:80%;
        margin:0 10%;
        text-transform: uppercase;
    }
    .btn-primary {
        .gradient-btn-primary();
        color:@clr-white;
    }
    .btn-facebook {
        .gradient-btn-facebook();
        color:@clr-white;

        .icon-facebook{
            float: left;
            font-size: 28px;
        }
    }
    .btn-default.btn-transparent {
        background: transparent;
        color:@clr-white;
        font-size: 20px;        
    }
    .btn-link {
        display: block;
        padding: 9px 3px;
        border: 1px solid @clr-green;
        .border-radius(24px);
        color: @clr-green;
        font-family: Roboto, Sans-serif, Arial;
        font-size: 18px;
        font-weight: 600;
        letter-spacing: 1px;
        text-align: center;
    }
    .btn-link:hover, .btn-link:focus {
        text-decoration: none;
    }     
    .btn-lg, .btn-group-lg > .btn {
        padding-top: 15px;
        padding-bottom: 16px;
        font-size: 24px;
        letter-spacing: 1px;
        line-height: 24px;
    }
    /***************************************************************************/


    /****************************************************************************
     $btn-back-to-top
     Description: CSS to represent a back-to-top button
    ****************************************************************************/
    .btn-back-to-top {
        display: block;
        padding: 0;
        margin: 0;
        width: 60px;
        height: 40px;
        .border-radius(6px, 6px, 0, 0);
        background-color: @clr-gray;
        background-attachment: scroll;
        background-image: url('../images/ics-gentera.png');
        background-position: center -187px;
        background-repeat: no-repeat;
        opacity: 0.9;
    }
    /***************************************************************************/ 


    /****************************************************************************
     $button-group 
     Description: CSS overrides to bootstrap's input-group class.
    ****************************************************************************/
    .button-group {
        display: block;
        position: relative;

        &:before, &:after {
            display: table;
            content: " ";
        }
        &:after {
            clear: both;
        }        

        .btn {
            display: inline-block;
            float: left;
            width: 50%;
            height: 55px;
            max-height: 55px;

            &:first-child {
                width: 45%;
                margin-right: 5%;
            }
        }
    }
    /***************************************************************************/ 


    /****************************************************************************
     $form-control
     Description: CSS overrides to bootstrap's form-control class.
    ****************************************************************************/
    .form-control {
        .border-radius-reset();
        .box-shadow-reset();
        .clr-white(border-color, 0.3);
        color:@clr-white;
        line-height: 1.42857143;
        font-weight: 700;

        &:focus {
            border-color: #06BACC;
        } 

        &::-webkit-input-placeholder {
            /* opacity: 0.5; */
            color:@clr-white !important;
            font-weight:100;
        }
        &:-moz-placeholder { /* Firefox 18- */
            /* opacity: 0.5; */
            color:@clr-white !important;
            font-weight:100;
        }
        &::-moz-placeholder {  /* Firefox 19+ */
            /* opacity: 0.5; */           
            color:@clr-white !important;
            font-weight:100;
        }

        /* control sizing */
        &.form-control-xl {
            height:75px;
            padding:12px 16px;
            font-size: 32px;
        }
        &.form-control-lg {
            height:66px;
            padding:21px 16px;
            font-size: 24px;
        }
        &.form-control-md {
            height:40px;
            padding:6px 16px;
            font-size: 16px;
        }
        &.form-control-sm {
            height:35px;
            padding:6px 16px;
            font-size: 14px;
        }        
        &.form-control-xs {
            height:30px;
            padding:6px 16px;
            font-size: 12px;
        }         
    }
    /***************************************************************************/


    /****************************************************************************
     $input-group 
     Description: CSS overrides to bootstrap's input-group class.
    ****************************************************************************/
    .input-group {
        .input-group-addon {
            padding: 0 1px;
            background-color: transparent;
            border-left:none;
            border-top:none;
            border-right:none;
            border-bottom:none;
            .border-radius-reset();
            color:@clr-white;
        }    
        .input-group-addon + .form-control {
            padding-top: 0px;
            padding-bottom: 0px;
        }
        .form-control {
            background-color: transparent;
            border-left:none;
            border-top:none;
            border-right:none;
            border-bottom:none;
            .border-radius-reset();
            .box-shadow-reset();
            color:@clr-white;
            font-size:24px;
            font-weight: 600;
        }
        .form-control:hover, 
        .form-control:focus,
        .form-control:active {
            .box-shadow-reset();
        }

        select.form-control {

            option {
                color: @clr-blueDark;
            }
            option:first-child {
                color: @clr-blueDark;
            }
        }
    }
    /***************************************************************************/


    /****************************************************************************
     $alert
     Description: CSS overrides to bootstrap's alert class.
    ****************************************************************************/
    .alert {
        display: block; 
        height: auto; 
        min-height: 60px;
        padding:0;
        border:none;
        .border-radius(0);

        &.fixed-top {
            position: absolute;
            z-index: 1;
            width:100%; 
        }
        &.alert-warning {
            .clr-orange(background-color, 0.2);
        }
        &.alert-success {
            .clr-greenMedium(background-color, 0.5);
        }
        .icomoon {
            display: block;
            position: absolute;
            z-index: 1;
            left:16px; top:16px;
            color:@clr-white;
            font-size:28px;
        }
        .icon-warning {}
        .icon-warning {}
        p {
            display: block;
            margin: 16px 0 16px 45px;
            color:@clr-white; 
            font-size: 18px;
            font-weight: 600;    
        }
        ol, ul {
            display: block;
            margin: 16px 0 16px 33px;
            color:@clr-white; 
            font-size: 18px;
            font-weight: 300;    
        }        
    }
    /***************************************************************************/     


    /****************************************************************************
     $navbar
     Description: CSS overrides to bootstrap's navbar class and descendants.
    ****************************************************************************/
    .navbar {
        position: relative;
        margin-bottom: 0;

        [class^="icon-"] {
            color:white;
            font-size: 22px;
            line-height: 64px;
        }        
    }
    .navbar-default {}
    .navbar-default .navbar-toggle {}

    .navbar-welcome {
        background-color: @clr-orange;
        border-color: @clr-orange;
    }
    .navbar-welcome .navbar-toggle,.navbar-step .navbar-toggle  {border-color: transparent;}
    .navbar-welcome .navbar-toggle .icon-bar,.navbar-step .navbar-toggle .icon-bar {background-color: @clr-white;}
    .navbar-step {background-color: @clr-bluenav;}
    .navbar-header {}
    .navbar-assets {
        display: block;
        position: static;
        padding: 0; margin: 0 15px 0 20px;
        height: 64px; width: auto;
    }
    .navbar-assets .navbar-heading {
        display: inline-block;
        overflow: hidden;
        float: left;
        margin: 0;
        width: 50%;
        color: @clr-white;
        font-family: 'Play';        
        font-size: 22px;
        font-weight: bold;
        line-height: 64px;
        text-overflow:ellipsis;
        white-space: nowrap;
    }
    .navbar-assets .navbar-tools {
        display: inline-block;
        float: right;
        margin: 0;
         width: auto;    
        list-style-type: none;   
        padding: 0;
    }
    .navbar-tools li {
        display: inline-block;
        padding: 0; margin: 0 0 0 8px;
        width:32px; height: 32px;     
    }
    .navbar-tools a {
        display: block;
        padding: 0; margin: 0;
        width: inherit; height: inherit; 
    }
    /***************************************************************************/    
    

    /****************************************************************************
     $navbar-toggle
     Description: CSS override to bootstrap's navbar toggle button on mobile.
    ****************************************************************************/
    .navbar-toggle {
        position: absolute;
        left: 0; top: 0;
        float: none;
        padding: 0;
        margin: 0;
        width: 42px;

        .icomoon {
            color:white;
            font-size: 26px;
            font-weight: bold;
            line-height: 62px;
            text-align: center;
        }        
    }
    .navbar-toggle .icon-bar {
      display: block;
      width: 6px;
      height: 6px;
      -webkit-border-radius:16px;
         -moz-border-radius:16px;
              border-radius:16px;

    }
    /***************************************************************************/


  

